# Ignore MacOS system files
.DS_Store

# Ignore temporary files generated by editors or IDEs
*.swp
*.bak
*.tmp
*.log

# Ignore deployment credentials or configuration
*.env
*.credentials

# Ignore Python cache files (if utility scripts are in Python)
__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore Terraform or CloudFormation temporary files
*.tfstate
*.tfstate.backup
*.template.json

# Ignore outputs folder if it contains generated files or logs
outputs/
